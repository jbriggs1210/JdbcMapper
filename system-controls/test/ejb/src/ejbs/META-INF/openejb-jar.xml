<?xml version="1.0"?>

<openejb-jar
        xmlns="http://www.openejb.org/xml/ns/openejb-jar-2.0"
        configId="org/apache/beehive/test"
        parentId="org/apache/geronimo/Server">

    <enterprise-beans>

        <session>
            <ejb-name>SimpleSessionBean</ejb-name>
            <jndi-name>SimpleSessionRemote</jndi-name>
            <jndi-name>org.apache.beehive.test.controls.system.controls.EjbLinkControl.jcx</jndi-name>
            <local-jndi-name>SimpleSessionLocal</local-jndi-name>
        </session>
        <entity>
            <ejb-name>SimpleEntityBean</ejb-name>
            <jndi-name>SimpleEntityRemote</jndi-name>
            <local-jndi-name>SimpleEntityLocal</local-jndi-name>
        </entity>

    </enterprise-beans>
</openejb-jar>