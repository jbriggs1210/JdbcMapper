package PF_ForwardMissingPath;

import org.apache.beehive.netui.pageflow.*;
import org.apache.beehive.netui.pageflow.annotations.Jpf;

/**
 *******************************************************************************
 *
 * @desc: This PageFlowController file declares a jpf:forward annotations in Forward
 * method "page1" but does not have either of the required attributes "path" or
 * "returnAction".
 *
 * @result: The compiler should catch this and issue an error.
 *
 ******************************************************************************/
@Jpf.Controller()
public class Controller extends PageFlowController
    {
    @Jpf.Action(
        forwards = {
            @Jpf.Forward(
                name = "begin",
                path = "Begin.jsp") 
        })
    protected Forward begin()
        {
        return new Forward("begin");
        }

    @Jpf.Action(
        forwards = {
            @Jpf.Forward(
                name = "page1") 
        })
    protected Forward page1()
        {
        return new Forward("page1");
        }
    }
