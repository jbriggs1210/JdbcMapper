package PF_MissingForwardJPF;

import org.apache.beehive.netui.pageflow.PageFlowController;
import org.apache.beehive.netui.pageflow.Forward;
import org.apache.beehive.netui.pageflow.annotations.Jpf;

/**
 * @desc: This jpf will define a Forward (which has a path to a jpf that does not
 *        exist) on an Action method.
 *
 * @result: Generate the appropriate warning, but do generate the config file.
 */
@Jpf.Controller()
public class Controller extends PageFlowController
{
    @Jpf.Action(
        forwards={
           @Jpf.Forward(name="foo", path="/DoesNotExist.jpf")
        }
    )
    protected Forward begin()
    {
        return new Forward("foo");
    }


}