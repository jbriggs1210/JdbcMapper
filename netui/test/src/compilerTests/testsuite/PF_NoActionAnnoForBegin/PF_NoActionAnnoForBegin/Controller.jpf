package PF_NoActionAnnoForBegin;

import org.apache.beehive.netui.pageflow.*;
import org.apache.beehive.netui.pageflow.annotations.Jpf;

/**
 *******************************************************************************
 *
 * @desc: This PageFlowController file contains a "begin" ActionForward method but no
 * "@jpf:action" annotation.
 *
 * @result: The compiler should catch this and issue an error.
 *
 ******************************************************************************/
@Jpf.Controller()
public class Controller extends PageFlowController
    {
    protected Forward begin()
        {
        return new Forward("pg1");
        }

    @Jpf.Action(
        forwards = {
            @Jpf.Forward(
                name = "pg1",
                path = "Pg1.jsp") 
        })
    protected Forward page1()
        {
        return new Forward("pg1");
        }
    }
