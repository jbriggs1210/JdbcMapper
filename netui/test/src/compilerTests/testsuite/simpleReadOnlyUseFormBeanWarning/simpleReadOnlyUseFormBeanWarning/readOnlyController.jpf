package simpleReadOnlyUseFormBeanWarning;

import javax.servlet.http.HttpSession;
import org.apache.beehive.netui.pageflow.PageFlowController;
import org.apache.beehive.netui.pageflow.Forward;
import org.apache.beehive.netui.pageflow.annotations.Jpf;

//This test verifies bug beehive-293

/**
 * This is the default controller for a blank web application.
 */
@Jpf.Controller(simpleActions={@Jpf.SimpleAction(name="submit", path="index.jsp",
                                                 useFormBean = "scopedBean",
                                                 readOnly = true,
                                                 outputFormBeanType = readOnlyController.SimpleBean.class)})

public class readOnlyController extends PageFlowController
{
private SimpleBean scopedBean = new SimpleBean();

public SimpleBean getScopedBean()
    {
        return scopedBean;
    }

    public void setScopedBean( SimpleBean _scopedBean )
    {
        scopedBean = _scopedBean;
    }

    
    @Jpf.Action(
        forwards={
           @Jpf.Forward(name="index", path="index.jsp")
        }
    )
    protected Forward begin()
    {
        return new Forward("index");
    }

    /**
     * Callback that is invoked when this controller instance is created.
     */
    protected void onCreate()
    {
    }

    /**
     * Callback that is invoked when this controller instance is destroyed.
     */
    protected void onDestroy(HttpSession session)
    {
    }

        

        
   
                                                
    public static class SimpleBean implements java.io.Serializable
    {
    }
}
