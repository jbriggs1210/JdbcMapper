      var attrVal = tag.getAttribute("netui:idScope");
   } catch (e) { /* ignore, in IE6 calling on a table results in an exception */ }
   if (attrVal != null)
      return attrVal;
   return getScopeId(tag.parentNode);
}

// lookup by tagId to "real id"
function lookupIdByTagId(id, tag)
{
